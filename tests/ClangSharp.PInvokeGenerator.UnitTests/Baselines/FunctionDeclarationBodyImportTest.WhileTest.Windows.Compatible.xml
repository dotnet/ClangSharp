<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<bindings>
  <namespace name="ClangSharp.Test">
    <class name="Methods" access="public" static="true">
      <function name="MyFunction" access="public" static="true">
        <type>int</type>
        <param name="count">
          <type>int</type>
        </param>
        <code>int i = 0;

      while (i &lt; count)
      {
          i++;
      }

      return i;</code>
      </function>
    </class>
  </namespace>
</bindings>
